<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="../../assets/ico/favicon.ico">
    <title>
    {% block title %}
    Dashboard Template for Bootstrap
    {% endblock %}
    </title>

    <!-- Bootstrap core CSS -->
    {% block stylesheets %}
    <link href="{{ asset('bundles/hospicesite/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/hospicesite/css/jquery.timepicker.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/hospicesite/css/bootstrap-datepicker.css') }}" rel="stylesheet">
  <!--  <link href="{{ asset('bundles/hospicesite/css/bootstrap-timepicker.min.css') }}" rel="stylesheet"> -->
    <!-- Custom styles for this template -->
    <link href="{{ asset('bundles/hospicesite/css/dashboard.css') }}" rel="stylesheet"/>
    <link href="{{ asset('bundles/hospicesite/css/chosen.min.css') }}" rel="stylesheet"/>
  <!--  <link href="{{ asset('bundles/hospicesite/css/datepicker.css') }}" rel="stylesheet"/> -->
    <link href="{{ asset('bundles/hospicesite/css/fullcalendar.min.css') }}" rel="stylesheet"/>
    <link href="{{ asset('bundles/hospicesite/css/fullcalendar.print.css') }}" rel='stylesheet' media='print'/>

    <link href="{{ asset('bundles/hospicesite/css/jquery-ui.min.css') }}" rel="stylesheet"/>
    <link href="{{ asset('bundles/hospicesite/css/jquery-ui.structure.min.css') }}" rel="stylesheet"/>
    <link href="{{ asset('bundles/hospicesite/css/jquery-ui.theme.min.css') }}" rel="stylesheet"/>
    {% endblock %}
  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Project name</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="#">Settings</a></li>
            <li><a href="{{ path('fos_user_profile_show') }}">Profile</a></li>
            <li><a href="{{ path('fos_user_security_logout') }}">Logout</a></li>
          </ul>
          <form class="navbar-form navbar-right">
            <input id="input_search" type="text" class="form-control" placeholder="Search...">
          </form>
        </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
          <ul class="nav nav-sidebar">
			<li><a href="{{ path('patient') }}">{% trans %}Patients{% endtrans %}</a></li>
			<li><a href="{{ path('volunteer') }}">{% trans %}Volunteers{% endtrans %}</a></li>
			<li><a href="{{ path('event') }}">{% trans %}Events{% endtrans %}</a></li>
          </ul>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
        {% block data_holder %}
        {% endblock %}
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="{{ asset('bundles/hospicesite/js/jquery.min.js') }}"></script>
    <!-- <script src="{{ asset('bundles/hospicesite/js/jquery-ui.min.js') }}"></script> -->
    <script src="{{ asset('bundles/hospicesite/js/chosen.jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/jquery.timepicker.min.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/jquery.datepair.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/datepair.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/bootstrap-datepicker.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/docs.min.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/moment.min.js') }}"></script>
    <script src="{{ asset('bundles/hospicesite/js/fullcalendar.min.js') }}"></script>

    <script type="text/javascript">
    $(document).ready(function() {    
        var config = {
            '.chosen-select'           : {width:"100%"},
            '.chosen-select-deselect'  : {allow_single_deselect:true},
            '.chosen-select-no-single' : {disable_search_threshold:10},
            '.chosen-select-no-results': {no_results_text:'Oops, nothing found!'},
            '.chosen-select-width'     : {width:"95%"}
        }
        for (var selector in config) {
            $(selector).chosen(config[selector]);
        }
    });
    </script>

    {% block javascripts %}
    {% endblock %}

    {% block bodypart %}
    {% endblock %}
  </body>
</html>
